<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>regdata</servlet-name>
        <servlet-class>com.statusreporting.regdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logcheck</servlet-name>
        <servlet-class>com.statusreporting.logcheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>statussend</servlet-name>
        <servlet-class>com.statusreporting.statussend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getdata</servlet-name>
        <servlet-class>com.statusreporting.getdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>excelsheet</servlet-name>
        <servlet-class>com.statusreporting.excelsheet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pdfgenerate</servlet-name>
        <servlet-class>com.statusreporting.pdfgenerate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>regdata</servlet-name>
        <url-pattern>/regdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logcheck</servlet-name>
        <url-pattern>/logcheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>statussend</servlet-name>
        <url-pattern>/statussend</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getdata</servlet-name>
        <url-pattern>/getdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>excelsheet</servlet-name>
        <url-pattern>/excelsheet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pdfgenerate</servlet-name>
        <url-pattern>/pdfgenerate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
           register.html
        </welcome-file>
    </welcome-file-list>
</web-app>
